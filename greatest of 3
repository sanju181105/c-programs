#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int a,b,c;
    cin>>a;
    cin>>b;
    cin>>c;
    if(a>b&&a>c)
    {
        cout<<a<<"id greater";
    }
    else if(b>a&&b>c)
    {
        cout<<b<<"ios greater";
    }
    else
    {
        cout<<c<<"is greater";
    }
    return 0;
}
